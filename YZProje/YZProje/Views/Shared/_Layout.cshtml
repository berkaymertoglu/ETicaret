                                        <img src="/assets/images/icon-svg/order-icon.svg" alt="Siparişler">
                                        <span class="icon-text text-style-1">16</span>
                                    </a>

                                    
                                <!-- navbar cart Ends -->
                            </div>
                        </nav>
                    </div>
                </div>
                <!-- main navbar Ends -->
            </div>
            <div class="overlay-7"></div>
