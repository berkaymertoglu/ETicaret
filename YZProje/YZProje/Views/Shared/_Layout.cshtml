                                        <li>
                                            <a href="#"><i class="mdi mdi-account"></i> @ViewBag.UserName</a>
                                        </li>
                                        <li>
                                            <a href="/Account/Logout"><i class="mdi mdi-logout"></i> Logout</a>
                                        </li>
                                    }
                                    else
                                    {
